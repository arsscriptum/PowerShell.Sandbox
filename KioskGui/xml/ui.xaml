        <Button x:Name="BtnBrowseFirefox" Content="..." HorizontalAlignment="Left" Margin="271,172,0,0" VerticalAlignment="Top" Width="35"/>

<Image Name="Img1" HorizontalAlignment="Left" Height="20" Margin="320,117,0,0" VerticalAlignment="Top" Width="20" Source="P:\GuiTemp\MyGui\img\check.png" Cursor="Arrow"/>
   
<Image Name="Img2" HorizontalAlignment="Left" Height="20" Margin="320,142,0,0" VerticalAlignment="Top" Width="20" Source="P:\GuiTemp\MyGui\img\cross.png" Cursor="Arrow"/>
        <Image Name="Img3" HorizontalAlignment="Left" Height="20" Margin="320,172,0,0" VerticalAlignment="Top" Width="20" Source="P:\GuiTemp\MyGui\img\check.png" Cursor="Arrow"/>
        

