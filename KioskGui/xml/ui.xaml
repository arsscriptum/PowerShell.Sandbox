        <TextBox x:Name="EdgeAppPath"  Text="" HorizontalAlignment="Left" Height="19" Margin="70,117,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="191"/>
        <TextBox x:Name="FirefoxAppPath"  Text="" HorizontalAlignment="Left" Height="19" Margin="70,172,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="191"/>
        
